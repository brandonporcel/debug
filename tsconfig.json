{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
        "checkJs": false,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
//   "include": [
//     "next-env.d.ts",
//     ".next/types/**/*.ts",
//     "**/*.ts",
//     "**/*.tsx"
// , "src/app/carousel-horizontal/page.jsx"  ],
  "include": [],
  "exclude": [
    "node_modules",
        "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
